{
  "name": "essai_rag_gros_doc_connecte",
  "nodes": [
    {
      "parameters": {
        "options": {
          "allowFileUploads": true
        }
      },
      "id": "chatTrigger",
      "name": "When chat message received",
      "type": "@n8n/n8n-nodes-langchain.chatTrigger",
      "typeVersion": 1,
      "position": [0, 0]
    },
    {
      "parameters": {
        "dataType": "binary",
        "loader": "docxLoader",
        "textSplittingMode": "custom"
      },
      "id": "dataLoader",
      "name": "Default Data Loader",
      "type": "@n8n/n8n-nodes-langchain.documentDefaultDataLoader",
      "typeVersion": 1,
      "position": [200, 0]
    },
    {
      "parameters": {
        "chunkSize": 1024,
        "chunkOverlap": 100
      },
      "id": "textSplitter",
      "name": "Recursive Character Text Splitter",
      "type": "@n8n/n8n-nodes-langchain.textSplitterRecursiveCharacterTextSplitter",
      "typeVersion": 1,
      "position": [400, 0]
    },
    {
      "parameters": {
        "functionCode": "return items.map(item => ({ json: { text: item.text } }));"
      },
      "id": "convertToMain",
      "name": "Convert to main",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [600, 0]
    },
    {
      "parameters": {
        "functionCode": "return items.filter(item => item.json.text.length < 3000);"
      },
      "id": "filterChunks",
      "name": "Filtrer chunks < 3000",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [800, 0]
    },
    {
      "parameters": {
        "model": "llama3.2:latest"
      },
      "id": "ollamaEmbeddings",
      "name": "Embeddings Ollama",
      "type": "@n8n/n8n-nodes-langchain.embeddingsOllama",
      "typeVersion": 1,
      "position": [1000, 0],
      "credentials": {
        "ollamaApi": {
          "id": "xHuYe0MDGOs9IpBW",
          "name": "Local Ollama service"
        }
      }
    },
    {
      "parameters": {
        "mode": "insert",
        "qdrantCollection": {
          "__rl": true,
          "value": "cdc",
          "mode": "id"
        }
      },
      "id": "qdrantInsert",
      "name": "Qdrant Vector Store",
      "type": "@n8n/n8n-nodes-langchain.vectorStoreQdrant",
      "typeVersion": 1,
      "position": [1200, 0],
      "credentials": {
        "qdrantApi": {
          "id": "sFfERYppMeBnFNeA",
          "name": "Local QdrantApi database"
        }
      }
    }
  ],
  "connections": {
    "chatTrigger": {
      "main": [
        [
          {
            "node": "dataLoader",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "dataLoader": {
      "ai_textSplitter": [
        [
          {
            "node": "textSplitter",
            "type": "ai_textSplitter",
            "index": 0
          }
        ]
      ],
      "ai_document": [
        [
          {
            "node": "qdrantInsert",
            "type": "ai_document",
            "index": 0
          }
        ]
      ]
    },
    "textSplitter": {
      "ai_textSplitter": [
        [
          {
            "node": "convertToMain",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "convertToMain": {
      "main": [
        [
          {
            "node": "filterChunks",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "filterChunks": {
      "main": [
        [
          {
            "node": "ollamaEmbeddings",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "ollamaEmbeddings": {
      "ai_embedding": [
        [
          {
            "node": "qdrantInsert",
            "type": "ai_embedding",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": true,
  "settings": {
    "executionOrder": "v1"
  }
}
